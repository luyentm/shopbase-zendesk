{
  "name": "Shopbase Integration",
  "author": {
    "name": "Tran Minh Luyen",
    "email": "admin@omegatics.com",
    "url": "https://www.omegatics.com"
  },
  "defaultLocale": "en",
  "private": false,
  "location": {
    "support": {
      "ticket_sidebar": "assets/iframe.html"
    }
  },
  "singleInstall": true,
  "version": "1.2",
  "frameworkVersion": "2.0",
  "parameters": [
    {
      "name": "domain",
      "type": "text",
      "required": true,
      "default": "https://"
    },
    {
      "name": "api_key",
      "type": "text",
      "required": true,
      "secure": true
    },
    {
      "name": "password",
      "type": "text",
      "required": true,
      "secure": true
    },
    {
      "name": "pattern",
      "type": "text",
      "required": true,
      "secure": true
    }
  ]
}